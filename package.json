{
  "name": "setup-sonar-action",
  "version": "0.0.0",
  "private": true,
  "description": "Create a project in SonarCloud using web api.",
  "main": "dist/index.js",
  "scripts": {
    "format": "prettier --write **/*.ts",
    "format:check": "prettier --check **/*.ts",
    "lint": "eslint --fix src/**/*.ts",
    "lint:check": "eslint src/**/*.ts",
    "build": "ncc build src/main.ts --license licenses.txt",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nelsoncanarinho/setup-sonar-action"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "axios": "^1.7.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-github": "^5.0.1",
    "eslint-plugin-jest": "^28.6.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "ts-jest": "^29.2.0",
    "typescript": "^5.5.3"
  }
}
